{
  "name": "ng-joanne-lee",
  "author": "Bob Lee <bob.bumsuk.lee@gmail.com>",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "tsc": "node_modules/.bin/tsc -p build/tsconfig.json",
    "clean": "rm -rf dist functions/dist",
    "clean:node": "rm -rf node_modules functions/node_modules",
    "build-prod": "ng build --prod --output-hashing none",
    "build": "yarn clean && yarn build-prod && yarn tsc && node dist/build/build.js",
    "build:ssr": "yarn build && ng build --prod --app 1 --output-hashing none && move dist/index.html functions/dist",
    "serve": "yarn build && firebase serve",
    "deploy": "yarn build && firebase deploy --only hosting",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.13",
    "@angular/common": "7.2.13",
    "@angular/compiler": "7.2.13",
    "@angular/core": "7.2.13",
    "@angular/forms": "7.2.13",
    "@angular/http": "7.2.13",
    "@angular/platform-browser": "7.2.13",
    "@angular/platform-browser-dynamic": "7.2.13",
    "@angular/router": "7.2.13",
    "core-js": "^2.4.1",
    "fs-extra": "^4.0.2",
    "ng-scrolltop": "^1.0.6",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.0",
    "uglify-js": "^3.1.3",
    "web-animations-js": "^2.3.1",
    "workbox-build": "^4.3.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "7.2.13",
    "@angular/language-service": "7.2.13",
    "@types/fs-extra": "^4.0.2",
    "@types/jasmine": "2.5.53",
    "@types/node": "^8.0.30",
    "@types/uglify-js": "^2.6.29",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "3.2.4"
  }
