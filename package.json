  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "tsc": "node_modules\\.bin\\tsc -p build\\tsconfig.json",
    "clean": "rm -rf dist functions\\dist",
    "clean:node": "rm -rf node_modules functions\\node_modules",
    "build": "ng build",
    "build-prod": "ng build --prod --output-hashing none",
    "build:workbox": "npm run clean && npm run build-prod && npm run tsc && node dist/build/build.js",
    "build:ssr": "npm run build:workbox && ng build --prod --app 1 --output-hashing none && move dist\\index.html functions\\dist",
    "serve": "firebase serve --only functions,hosting",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
